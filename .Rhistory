area<-function(x=0){
z<-seq(-3,3,0.01)
fdp<-dnorm(z)
plot(z,fdp,type="l")
polygon(c(z[z<=x],x),c(fdp[z<=x],fdp[z==x]),col="red")
}
area()
polygon(c(z[z<=x],x),c(fdp[z<=x],fdp[z==-3]),col="red")
